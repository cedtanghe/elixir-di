{
    "name": "elixir/elixir-di",
    "description": "Component dependency injection",
    "version": "1.0.0",
	"minimum-stability": "dev",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "di"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-di",
    "repositories": [
    ],
    "require": {
        "php": ">=5.6",
        "elixir/elixir-dispatcher": "*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\DI\\": "tests/"
        }
    },
    "suggest": {
        "container-interop/container-interop": "Container Interoperability"
    }
}